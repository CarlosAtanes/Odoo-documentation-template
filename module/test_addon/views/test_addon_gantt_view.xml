<!-- test_addon_gantt_view.xml -->

<odoo>
    <record id="view_gantt_test_addon" model="ir.ui.view">
        <field name="name">test.addon.gantt</field>
        <field name="model">test.addon.model</field>
        <field name="arch" type="xml">
            <gantt string="Test Addon Gantt"
                   date_start="start_date"
                   date_stop="end_date">
                <field name="name"/>
                <field name="description"/>
            </gantt>
        </field>
    </record>
</odoo>
